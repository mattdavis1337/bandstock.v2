<h2>Show Tile</h2>

<ul>

  <li>
    <strong>Hash:</strong>
    <%= @tile.hash %>
  </li>

  <li>
    <strong>Location X</strong>
    <%= @tile.loc_x %>
  </li>

  <li>
    <strong>Location Y</strong>
    <%= @tile.loc_x %>
  </li>

  <li>
    <strong>Size X</strong>
    <%= @tile.loc_x %>
  </li>

  <li>
    <strong>Size X</strong>
    <%= @tile.loc_x %>
  </li>

  <li>
    <strong>Image:</strong>
    <%= @tile.image %>
  </li>

</ul>

<span><%= link "Edit", to: tile_path(@conn, :edit, @tile) %></span>
<span><%= link "Back", to: tile_path(@conn, :index) %></span>
